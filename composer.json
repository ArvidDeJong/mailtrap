{
    "name": "darvis/mailtrap",
    "description": "A Laravel package for Mailtrap integration",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arvid de Jong",
            "email": "info@arvid.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Darvis\\Mailtrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darvis\\Mailtrap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darvis\\Mailtrap\\MailtrapServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}